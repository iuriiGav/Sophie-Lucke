{
  "name": "sophie-lucke-website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --watch --mode development",
    "prod": "webpack --mode production",
    "compile:sass": "node-sass sass/main.scss css/style.css -w",
    "prefix:css": "postcss css/style.css --use autoprefixer -o css/style.css",
    "copyfiles": "copyfiles *.js *.html css/style.css img/galery/*.jpg img/sophie-cover-pictures/*.jpg /dist img/*.jpg img/*.png Sophie-Lucke",
    "webpack:build": "webpack",
    "deploy": "gh-pages -d Sophie-Lucke",
    "build:local": "npm run prefix:css && npm run webpack:build && npm run copyfiles",
    "watch:pug": "pug -w ./pug-src/index.pug ./pug-src/concerts.pug ./pug-src/biography.pug ./pug-src/news.pug ./pug-src/listen.pug ./pug-src/gallery.pug -o ./test-pug -P"
  },
  "browserslist": "last 10 versions",
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/iuriiGav/Sophie-Lucke",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "flickity": "^2.2.2",
    "gh-pages": "^3.1.0",
    "jquery": "^3.5.1",
    "postcss-cli": "^8.3.1",
    "swiper": "^6.4.11"
  }
}
